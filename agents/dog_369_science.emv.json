          "properties": {
            "dominant_frequency": {"type": "number"},
            "spectral_centroid": {"type": "number"},
            "rms_energy": {"type": "number"},
            "vocalization_type": {"type": "string", "enum": ["bark", "growl", "whine", "howl", "yelp", "none"]},
            "bark_type": {"type": "string", "enum": ["alert", "play", "anxiety", "demand", "aggressive", "none"]},
            "duration_ms": {"type": "number"}
          }
        },
        "visual_data": {
          "type": "object",
          "description": "Motion and posture features from dog-emotion-engine",
          "properties": {
            "movement_speed": {"type": "number"},
            "movement_direction": {"type": "string"},
            "detected_patterns": {"type": "array", "items": {"type": "string"}},
            "bounding_box": {"type": "object"},
            "size_change_rate": {"type": "number"}
          }
        },
